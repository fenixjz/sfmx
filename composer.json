{
    "name": "fenixjz/sfmx",
    "description": "SoftMetrix PHP Framework Version 2 - Development Package",
    "keywords": [
        "sfmx",
        "framework"
    ],
    "homepage": "http://softmetrixgroup.com/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cold Fire",
            "email": "fenixjz@gmail.com",
            "homepage": "http://softmetrixgroup.com/",
            "role": "Core framework development"
        },
        {
            "name": "Marko Medojevic",
            "email": "marko.medojevic@softmetrixgroup.com",
            "homepage": "http://softmetrixgroup.com/",
            "role": "Core framework development"
        }
    ],
    "support": {
        "issues": "https://github.com/fenixjz/sfmx/issues?state=open",
        "wiki": "https://github.com/fenixjz/sfmx/wiki",
        "source": "https://github.com/fenixjz/sfmx"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "yiisoft/yii2": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "yiisoft/yii2-composer": "~2.0.4",
        "ezyang/htmlpurifier": "~4.6",
        "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0",
        "bower-asset/jquery": "3.4.*@stable | 3.3.*@stable | 3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/inputmask": "~3.2.2 | ~3.3.5",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "~2.0.1",
        "ext-gd": "*",
        "ext-exif": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "cebe/indent": "~1.0.2",
        "friendsofphp/php-cs-fixer": "~2.2.3",
        "johnkary/phpunit-speedtrap": "^1.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "suggest": {
        "yiisoft/yii2-coding-standards": "you can use this package to check for code style issues when contributing to yii"
    },
    "autoload": {
        "psr-4": {
            "yii\\": "framework/",
            "yii\\cs\\": "cs/src/"
        }
    },
    "config": {
        "platform": {"php": "5.4"}
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
